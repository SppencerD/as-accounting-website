{
  "version": 2,
  "builds": [
    { "src": "dist/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/dist/index.html" },
    { "src": "/en", "dest": "/dist/en/index.html" },
    { "src": "/en/(.*)", "dest": "/dist/en/$1" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}


